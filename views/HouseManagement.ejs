<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet"
        href="https://fonts.googleapis.com/css2?family=Material+Symbols+Sharp:opsz,wght,FILL,GRAD@48,500,0,0" />
    <link rel="stylesheet" href="/css/manageHouse.css">
    <title>NHÀ TRỌ CỦA TÔI</title>
</head>

<body>
    <div class="container">
        <aside>
            <div class="top">
                <div class="logo">
                    <img src="/image/logo.png" alt="">
                    <h2>i<span class="danger">HOME</span></h2>
                </div>
                <div class="close" id="close-btn">
                    <span class="material-symbols-sharp">
                        close
                    </span>
                </div>
            </div>

            <div class="sidebar">
                <a href="/">
                    <span class="material-symbols-sharp">
                        grid_view
                    </span>
                    <h3>Trang chủ</h3>
                </a>
                <a href="/HouseManagement">
                    <span class="material-symbols-sharp">
                        home
                    </span>
                    <h3>Quản lý nhà</h3>
                </a>
                <a href="/RoomManagement">
                    <span class="material-symbols-sharp">
                        bed
                    </span>
                    <h3>Quản lý phòng</h3>
                </a>
                <a href="/TenantsManagement">
                    <span class="material-symbols-sharp">
                        person_outline
                    </span>
                    <h3>Quản lý người thuê</h3>
                </a>
                <a href="/ContractManagement">
                    <span class="material-symbols-sharp">
                        gavel
                    </span>
                    <h3>Quản lý hợp đồng</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-sharp">
                        room_service
                    </span>
                    <h3>Quản lý dịch vụ</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-sharp">
                        electric_bolt
                    </span>
                    <h3>Chỉ số điện</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-sharp">
                        invert_colors
                    </span>
                    <h3>Chỉ số nước</h3>
                </a>
                <a href="/ReceiptManagement">
                    <span class="material-symbols-sharp">
                        receipt_long
                    </span>
                    <h3>Hóa đơn</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-sharp">
                        dangerous
                    </span>
                    <h3>Báo cáo sự cố</h3>
                </a>
                <a href="#">
                    <span class="material-symbols-sharp">
                        devices
                    </span>
                    <h3>Tài sản</h3>
                </a>
                <a href="">
                    <form action="/logout" method="POST" class="btn-logout">
                        <span class="material-symbols-sharp">
                            logout
                        </span>
                        <button type="submit">
                            <h3>Đăng xuất</h3>
                        </button>
                    </form>
                </a>
            </div>
        </aside>

        <main>
            <h1>Quản lý nhà</h1>

            <div class="date" id="curent-day">
                <input type="date">
            </div>

            <div class="recent-orders">
                <a href="/HouseManagement/addhome">
                    <button class="btn-addhouse">
                        Tạo nhà mới
                    </button>
                </a>
                <h2>Danh sách khu nhà thuê</h2>
                <table>
                    <thead>
                        <th>STT</th>
                        <th>Tên nhà trọ</th>
                        <th>Số nhà</th>
                        <th>Trạng thái</th>
                        <th>Hành động</th>
                        <th>Hành động</th>
                    </thead>
                    <tbody>
                        <% for(var i=0;i<nhatro.length;i++){%>
                            <tr>
                                <td><%=i+1%></td>
                                <td>
                                    <%=nhatro[i].TenNhaTro%>
                                </td>
                                <td>
                                    <%=nhatro[i].DiaChi%>
                                </td>
                                <td class="warning">Đang hoạt động</td>
                                <td>
                                    <a href="/HouseManagement/edithome/<%=nhatro[i].idNhaTro%>" class="primary">
                                        Sửa
                                    </a>
                                </td>
                                <td>
                                    <a href="/HouseManagement/deletehome/<%=nhatro[i].idNhaTro%>" class="danger">
                                        Xóa
                                    </a>
                                </td>
                            </tr>
                            <% } %>
                    </tbody>
                </table>
            </div>
        </main>

        <div class="right">
            <div class="top">
                <button id="menu-btn">
                    <span class="material-symbols-sharp">
                        menu
                    </span>
                </button>
                <div class="theme-toggler">
                    <span class="material-symbols-sharp active">
                        light_mode
                    </span>
                    <span class="material-symbols-sharp">
                        dark_mode
                    </span>
                </div>
                <div class="profile">
                    <div class="info">
                        <p>Xin chào, <b>Phát</b></p>
                        <small class="text-muted">User</small>
                    </div>
                    <div class="profile-photo">
                        <img src="/image/profile-1.jpg" alt="">
                    </div>
                </div>
            </div>

            <div class="recent-updates">
                <h2>Thông báo</h2>
                <div class="updates">
                    <div class="update">
                        <div class="profile-photo">
                            <img src="/image/profile-2.jpg" alt="">
                        </div>
                        <div class="message">
                            <p><b>Quang Dũng</b> đã thông báo cho bạn một sự cố!!!</p>
                            <small class="text-muted">2 Phút trước</small>
                        </div>
                    </div>
                    <div class="update">
                        <div class="profile-photo">
                            <img src="/image/profile-3.jpg" alt="">
                        </div>
                        <div class="message">
                            <p><b>Hương Giang</b> đã thông báo cho bạn một sự cố!!!</p>
                            <small class="text-muted">10 Phút trước</small>
                        </div>
                    </div>
                    <div class="update">
                        <div class="profile-photo">
                            <img src="/image/profile-4.jpg" alt="">
                        </div>
                        <div class="message">
                            <p><b>Đắc Trung</b> đã thông báo cho bạn một sự cố!!!</p>
                            <small class="text-muted">14 Phút trước</small>
                        </div>
                    </div>
                </div>
            </div>

            <div class="sales-analytics">
                <h2>Phân tích</h2>
                <div class="item online">
                    <div class="icon">
                        <span class="material-symbols-sharp">
                            payments
                        </span>
                    </div>
                    <div class="data">
                        <div class="info">
                            <h3>Tổng doanh thu so với năm trước</h3>
                            <small class="text-muted">24 giờ qua</small>
                        </div>
                        <h2 class="success">+39%</h2>

                    </div>
                </div>
                <div class="item offline">
                    <div class="icon">
                        <span class="material-symbols-sharp">
                            meeting_room
                        </span>
                    </div>
                    <div class="data">
                        <div class="info">
                            <h3>Tổng số lượng phòng thuê so với năm trước</h3>
                            <small class="text-muted">24 giờ qua</small>
                        </div>
                        <h2 class="danger">-10%</h2>
                    </div>
                </div>
                <div class="item customer">
                    <div class="icon">
                        <span class="material-symbols-sharp">
                            person
                        </span>
                    </div>
                    <div class="data">
                        <div class="info">
                            <h3>Khách thuê mới</h3>
                            <small class="text-muted">24 giờ qua</small>
                        </div>
                        <h2 class="success">0%</h2>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <script src="/js/dung.js"></script>
</body>

</html>